<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">

    <application>mwthr-com</application>

    <threadsafe>true</threadsafe>

    <static-files>
        <include path="/favicon.ico" expiration="7d" />
        <include path="/robots.txt" expiration="7d" />
        <include path="/css/**" expiration="1d" />
        <include path="/images/**" expiration="7d" />
        <include path="/js/**" expiration="1d" />
    </static-files>

    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/logging.properties" />
    </system-properties>

    <version>1</version>

</appengine-web-app>
